<h2 class="page-header">Search Book</h2>
<form action="/books" method="GET">
    <div class="form-row">
      <div class="form-item">
          <label>Title</label><br/>
          <input type="text" name="title" value="<%=searchOptions.title%>" ></input>
      </div>
   </div>
   
   <div class="form-row">
      <div class="form-item">
        <label>Published After</label><br/>
        <input type="date" name="publishAfter" value="<%=searchOptions.publishedAfter%>" ></input>
      </div>
    
      <div class="form-item">
        <label>Published Before</label><br/>
        <input type="date" name="publishedBefore" value="<%=searchOptions.publishedBefore%>" ></input>
      </div>
    </div>
    <div class="form-row form-row-end">
      <button class="btn btn-primary" type="submit">Search</button>
    </div>
</form>
<br>
<%- include("../partials/bookGrid",{books:books,large:true}) %> 
